mysqldump -u root -p budget --routines > dump_budget.sql
mysqldump -u root -p budget > dump_budget.sql
mysqldump -u root -p --no-data budget > dump_budget.sql
mysqldump -u root -p --no-create-info budget > dump_budget.sql


